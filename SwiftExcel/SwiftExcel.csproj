<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors>Roman Pavelko</Authors>
    <PackageLicenseFile>license.md</PackageLicenseFile>
    <PackageProjectUrl>https://swiftexcel.azurewebsites.net/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/RomanPavelko/SwiftExcel</RepositoryUrl>
    <Copyright>2019 SwiftExcel Roman Pavelko</Copyright>
    <PackageTags>Excel Writer ExcelWriter ExcelOpenXML</PackageTags>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\license.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
